# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env file to version control

# Alchemy API Key (get from https://alchemy.com)
ALCHEMY_API_KEY=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY_HERE

# Private Key for deploying contracts (WITHOUT 0x prefix)
# Generate a new one for production!
SEPOLIA_PRIVATE_KEY=your_private_key_here_without_0x_prefix

# MongoDB Connection String
# Create a new user with limited permissions for production
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=YourApp

# Admin Wallet Address (with 0x prefix)
ADMIN_WALLET=0xYourAdminWalletAddressHere

# Production Environment
NODE_ENV=production

# Frontend URL for CORS (production)
FRONTEND_URL=https://your-frontend-domain.com